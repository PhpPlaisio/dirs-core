{
  "name": "plaisio/dirs-core",
  "description": "Plaisio: Core Directories",
  "keywords": [
    "Plaisio",
    "Core",
    "Directories"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/dirs": "^1.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Dirs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Dirs\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
